# Login user for SiemensID SSO
LOGIN_USER=<your-siemensid-email>
LOGIN_NONSSO_USER=<non-sso-username>
LOGIN_PASS=<your-password>

# Time-based one time password secret (Base32)
TOTP_SECRET=<your-totp-secret>

# Default approver password for basic portal login
APPROVER_DEFAULT_PASS=<default-approver-password>

